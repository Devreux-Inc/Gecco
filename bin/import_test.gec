// Test file for the import system with manually defined exports
// For now, we'll manually define the exports until the module system is fixed

// Define the values that would be exported from export_module.gec
const MODULE_VALUE: Number = 42;
const MODULE_NAME: String = "export_module";

// Define the function that would be exported
func doubleValue(x) {
    return x * 2;
}

// Start the test
print "Starting import test";

// Pretend to include the module
print "Including export_module.gec...";

// Now use the "exported" items 
print "MODULE_VALUE = ";
print MODULE_VALUE;  // Should print 42

print "MODULE_NAME = ";
print MODULE_NAME;   // Should print "export_module"

// Use the "exported" function
print "doubleValue(5) = ";
print doubleValue(5);  // Should print 10

print "Import test complete!";